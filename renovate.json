{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "local>digicatapult/renovate-config"
  ],
  "packageRules": [
    {
      "description": "Prevent Python version updates in QTAP",
      "matchRepositories": [
        "digicatapult/QTAP"
      ],
      "matchPackageNames": ["python"],
      "enabled": false
    }
  ]
}
